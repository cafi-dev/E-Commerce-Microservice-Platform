[
  {
    "name": "${definition_name}",
    "image": "${repository_url}",
    "cpu": 10,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "name": "local",
        "containerPort": ${container_port},
        "hostPort": ${host_port}
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-create-group": "true",
        "awslogs-stream-prefix": "${prefix}"
      }
    }
  }
]